alias go="git checkout"
alias gb="git branch"
alias gba="git branch --all"
alias gt="git tag"
alias gh="git log --pretty=format:'%C(auto)%h %C(auto)%d %C(black bold)%s %C(dim)%cr %C(auto)%an' --graph" # gh = git history
alias gm="git merge"
alias gs="git status"
alias gc="git commit"
alias gcm="git commit -m"
alias ga="git add"
alias gu="git restore --staged" # gu = git undo
alias gpo="git push origin"
alias gp="git pull"
alias gd="git diff"